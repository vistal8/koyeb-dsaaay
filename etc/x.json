{
	"inbounds": [{
			"port": 3333,
			"listen": "127.0.0.1",
			"protocol": "vmess",
			"settings": {
				"clients": [{
					"id": "$AUUID"
				}]
			},
			"streamSettings": {
				"network": "ws",
				"wsSettings": {
					"path": "/vmess"
				}
			}
		},
		{
			"listen": "/etc/caddy/vless",
			"protocol": "vless",
			"settings": {
				"clients": [{
					"id": "$AUUID"
				}],
				"decryption": "none"
			},
			"streamSettings": {
				"network": "ws",
				"wsSettings": {
					"path": "/$AUUID-vless"
				}
			}
		},
		{
			"listen": "/etc/caddy/trojan",
			"protocol": "trojan",
			"settings": {
				"clients": [{
					"password": "$AUUID"
				}]
			},
			"streamSettings": {
				"network": "ws",
				"wsSettings": {
					"path": "/$AUUID-trojan"
				}
			}
		}
	],
	"outbounds": [{
		"protocol": "freedom"
	}]
}